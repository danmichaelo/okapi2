{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "biblio": {
    "dataSource": "es-okapi",
    "public": true,
    "log": "trace",
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "query": true
        }
      }
    }
  },
  "authority": {
    "dataSource": "es-authority",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "complete": true
        }
      }
    }
  }
}
